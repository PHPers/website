<body class="{% if page.homepage %}homepage{% else %} {% endif %}{% if page.body_class %}{{ page.body_class }}{% endif %}">
    <!--<body class="navigation-is-open">-->
    <!--[if lt IE 8]>
    <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade
        your browser</a> to improve your experience.</p>
    <![endif]-->

    {% block intro %}{% endblock %}

    <div id="sticky-anchor"></div>
    <header>
        <div class="row">
            <div class="small-4 columns">
                <a href="/">
                    <img src="/images/logo.svg" alt="" onerror="this.onerror=null; this.src='/images/logo.png'">
                </a>
            </div>
            <div class="small-8 columns menu hide">
                <a href="#nav" class="nav-trigger js-nav-trigger">
                    <span class="nav-icon"></span>
                    <span class="nav-sep"></span>
                <span class="nav-text js-nav-text" data-toggle-text="Close" data-opened="Close"
                      data-closed="Menu">Menu</span>
                </a>
            </div>
        </div>
        {% if page.show_filters %}
            <div id="filters" class="clearfix">
                <div class="filterWrapper">
                    <div class="filter" data-type="city">
                        <div class="selected">
                            <span class="pix">
                                <i class="fa fa-angle-down"></i>
                            </span>
                            <strong class="hidden-for-small-only">Miasto</strong>
                            <strong class="visible-for-small-only"><i class="fa fa-map-marker"></i></strong>:
                            <span class="txt"></span>
                        </div>
                        <ul class="menu-dropdown">
                            <li data-id="all" class="default"><span class="link">Wszystkie</span></li>
                            <li data-id="trojmiasto" ><span class="link">Trójmiast</span></li>
                            <li data-id="poznan" ><span class="link">Poznań</span></li>
                            <li data-id="warszawa" ><span class="link">Warszawa</span></li>
                            <li data-id="krakow" ><span class="link">Kraków</span></li>
                        </ul>
                    </div>
                </div>
                <div class="filterWrapper">
                    <div class="filter" data-type="month">
                        <div class="selected">
                            <span class="pix">
                                <i class="fa fa-angle-down"></i>
                            </span>
                            <strong class="hidden-for-small-only">Kiedy</strong>
                            <strong class="visible-for-small-only"><i class="fa fa-calendar"></i></strong>:
                            <span class="txt" ></span>
                        </div>
                        <ul class="menu-dropdown">
                            <li data-id="all" class="default"><span class="link">Wszystkie</span></li>
                            <li data-id="2015-11" ><span class="link">Listopad 2015</span></li>
                            <li data-id="2015-10" ><span class="link">Październik 2015</span></li>
                            <li data-id="2015-09" ><span class="link">Wrzesień 2015</span></li>
                        </ul>
                    </div>
                </div>
            </div>
        {% endif %}
    </header>
    <main>

        {% block content_wrapper %}
            {% block content %}{% endblock %}
        {% endblock %}

        {% if not page.disable_newsletter %}
            <section id="newsletter" class="alternative newsletter">
                <div class="row">
                    <div class="small-12 medium-7 large-8 columns">
                        <h3 class="headline">Bądź na bieżąco</h3>
                    </div>
                </div>
                <div class="row">
                    <div class="small-12 medium-10 columns">
                        <div class="form-wrap newsletter-cont" data-perspective>
                            <div id="newsletterFormWrap" class="newsletter-wrap">
                                <div class="content">
                                    <form id="newsletterForm" data-wrap="newsletterFormWrap" class="row collapse">
                                        <div class="small-12 medium-9 columns">
                                            <input type="email" placeholder="Subskrybuj aktualizacje" name="subscriber_email">
                                        </div>
                                        <div class="small-12 medium-3 columns">
                                            <button type="submit" class="button postfix elephant-btn elephant-slide-left-btn">
                                                Zapisz się
                                            </button>
                                        </div>
                                    </form>
                                </div>
                                <div class="side">
                                    <div class="row">
                                        <div class="small-2 columns text-right">
                                        <span class="h1">
                                            <i class="fa fa-smile-o"></i>
                                        </span>
                                        </div>
                                        <div class="small-10 columns">
                                        <span class="h4">
                                            Thank you!
                                        </span>

                                            <p>Your message has been successfully sent. We will contact you very soon!</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        {% endif %}
        {% if not page.disable_footer %}
            <footer class="footer">
                <div class="row">
                    <div class="small-12 medium-7 medium-push-5 columns">
                        <div class="row collapse">
                            <div class="small-12">
                                <p class="logo show-for-small-only"><img src="/images/favicon.ico"> PHPers</p>
                            </div>
                            <div class="small-12 medium-4 columns">
                                <h5>Chcesz pomóc</h5>
                            </div>
                            <div class="small-12 medium-8 columns">
                                <p>
                                    Masz pomysł na prelekcję? Zgłoś ją przez <a href="#">formularz</a>.
                                    <br>
                                    Chcesz zorganizować spotkanie lub
                                    być sponsorem
                                    naszej działalności? Przeczytaj <a href="#">FAQ</a>.
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="small-12 medium-5 medium-pull-7 columns">
                        <p class="logo hide-for-small-only">
                            <a href="/">
                            <img src="/images/favicon.ico"> PHPers
                            </a>
                        </p>

                        <p class="footer-links">
                            <a href="/idea-spotkan">O projekcie</a>
                            <a href="/idea-spotkan#organizatorzy">Organizatorzy</a>
                            <a href="/meetup">Spotkania</a>
                            <a href="/faq">FAQ</a>
                            <a href="/#zglos-sie">Zgłoś się!</a>
                            <a href="/napisz-do-nas">Kontakt</a>
                        </p>
                        <ul class="inline-list social">
                            <a href="http://facebook.com/PHPersPL" target="_blank"><i class="fa fa-facebook"></i></a>
                            <a href="http://twitter.com/PHPersPL" target="_blank"><i class="fa fa-twitter"></i></a>
                            <a href="https://www.youtube.com/user/PHPersPL" target="_blank"><i class="fa fa-youtube"></i></a>
                        </ul>
                    </div>
                </div>
            </footer>

            <section class="copyright">
                <div class="row">
                    <div class="small-12 medium-4 medium-push-8 columns thanks-for">
                        by <a href="https://twitter.com/tekla_to">@tekla_to</a>
                    </div>
                    <div class="small-12 medium-8 medium-pull-4 columns">
                        <i class="fa fa-copyright"></i> Copyright <span class="phpers">phpers</span>
                        All rights reserved.
                    </div>
                </div>
            </section>
        {% endif %}
    </main>

    <div id="nav" class="nav">
        <div class="row navigation-wrapper">
            <div class="small-6 columns">
                <nav>
                    <ul class="primary-nav">
                        <li><a href="/">Home</a></li>
                        <li><a href="/idea-spotkan">O projekcie</a></li>
                        <li><a href="/idea-spotkan#organizatorzy">Organizatorzy</a></li>
                        <li><a href="/meetup">Spotkania</a></li>
                        <li><a href="/faq">FAQ</a></li>
                        <li><a href="#zglos-sie">Zgłoś się!</a></li>
                        <li><a href="/napisz-do-nas">Kontakt</a></li>
                    </ul>
                </nav>
            </div>
            <div class="small-3 small-pull-1 columns text-right">
                <div class="row secondary-nav">
                    <div class="small-4 medium-12 columns">
                        <a href="http://facebook.com/PHPersPL" target="_blank">
                            <i class="fa fa-facebook"></i>
                            <span class="hide-for-small">Facebook</span>
                        </a>
                    </div>
                    <div class="small-4 medium-12 columns">
                        <a href="http://twitter.com/PHPersPL" target="_blank">
                            <i class="fa fa-twitter"></i>
                            <span class="hide-for-small">Twitter</span>
                        </a>
                    </div>
                    <div class="small-4 medium-12 columns">
                        <a href="https://www.youtube.com/user/PHPersPL" target="_blank">
                            <i class="fa fa-youtube"></i>
                            <span class="hide-for-small">YouTube</span>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
