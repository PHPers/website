    <script>
        $(document).foundation();
    </script>

    <script>
        //    $(window).stellar();
    </script>

    <script>
        $(".owl-carousel").owlCarousel({
            items: 2,
            itemsDesktop: [1025, 4],
            itemsDesktopSmall: [641, 3],
            lazyLoad: false,
            navigation: false,
            pagination: true,
            autoPlay: 8000,
            stopOnHover: true
        });
    </script>

    <script>window.viewportUnitsBuggyfill.init();</script>


    <script type="text/javascript">
        jQuery(function ($) {

            if (!Modernizr.mobile) {
                // start all the timers
                $('.timer').each(count);
                // restart a timer when a button is clicked
                function count(options) {
                    var $this = $(this);
                    options = $.extend({}, options || {}, $this.data('countToOptions') || {});
                    $this.countTo(options);
                }
            }
        });
    </script>
    <script language="javascript">

        var $map = $('#map');
        if ($map.length) {
            var markerStyle = {image: '/images/elephpant-small.png',};
            var regionStyle = {
                fill: 'white',
                stroke: 'gray',
                "stroke-width": 1,
                "stroke-opacity": 1,
                cursor: 'arrow'
            };

            var phpers = [
                {latLng: [52.13, 21.00], name: 'Warszawa', slug: 'warszawa', style: markerStyle},
                {latLng: [54.20, 18.38], name: 'Trójmiasto', slug: 'trojmiasto', style: markerStyle},
                {latLng: [50.22, 18.40], name: 'Śląsk', slug: 'slask', style: markerStyle},
                {latLng: [51.06, 17.01], name: 'Wrocław', slug: 'wroclaw', style: markerStyle},
                {latLng: [49.93, 19.56], name: 'Kraków', slug: 'krakow', style: markerStyle},
                {latLng: [53.01, 18.36], name: 'Toruń', slug: 'torun', style: markerStyle},
                {latLng: [52.24, 16.56], name: 'Poznań', slug: 'poznan', style: markerStyle},
                {latLng: [53.46, 20.28], name: 'Olsztyn', slug: 'olsztyn', style: markerStyle},
                {latLng: [51.77, 19.48], name: 'Łódź', slug: 'lodz', style: markerStyle},
            ];

            $('#map').vectorMap({
                map: 'pl_merc_en',
                zoomButtons: false,
                zoomOnScroll: false,
                backgroundColor: '#f9f9f9',
                labels: {
                    markers: {
                        render: function (i) {
                            return phpers[i].name;
                        },
                    },
                },
                regionStyle: {
                    initial: regionStyle,
                    hover: regionStyle,
                    selected: regionStyle,
                },
                markers: phpers,
                onRegionTipShow: function (e, el, code) {
                    e.preventDefault();
                },
                onMarkerTipShow: function (e, el, code) {
                    e.preventDefault();
                    console.log(e, el, code);
                    console.log(phpers[code]);
                },
                onMarkerClick: function (e, code) {
                    var url = document.location.protocol + '//' + document.domain;
                    if (document.location.port) {
                        url += ':' + document.location.port;
                    }
                    url += '/meetup/?c=' + phpers[code].slug;
                    window.location = url;
                }
            });
        }
    </script>

    <script src="/js/main.js"></script>

    <script type="text/javascript">
        var $counter = $('#counter');
        if ($counter.length) {
            var datetime = $counter.data('date');
            var time = $counter.data('time');
            if (time) {
                datetime = datetime.concat(" ").concat(time);
            }
            $('#counter').countdown(datetime).on('update.countdown', function (event) {
                $("#days").text(event.offset.totalDays);
                $("#hours").text(event.offset.hours);
                $("#minutes").text(event.offset.minutes);
                $("#seconds").text(event.offset.seconds);
            });
        }
    </script>

    <script>
        var mapCont = document.getElementById('locationMap');
        if (mapCont) {
            var venue_location = mapCont.getAttribute('data-latlng').replace(" ", "").split(',');
            var lat = venue_location[0];
            var lng = venue_location[1];
            var map = new GMaps({
                scrollwheel: false,
                el: '#locationMap',
                lat: lat,
                lng: lng,
                styles: [{"featureType":"administrative","elementType":"labels.text.fill","stylers":[{"color":"#444444"}]},{"featureType":"landscape","elementType":"all","stylers":[{"color":"#f2f2f2"}]},{"featureType":"poi","elementType":"all","stylers":[{"visibility":"off"}]},{"featureType":"road","elementType":"all","stylers":[{"saturation":-100},{"lightness":45}]},{"featureType":"road.highway","elementType":"all","stylers":[{"visibility":"simplified"}]},{"featureType":"road.arterial","elementType":"labels.icon","stylers":[{"visibility":"off"}]},{"featureType":"transit","elementType":"all","stylers":[{"visibility":"off"}]},{"featureType":"water","elementType":"all","stylers":[{"color":"#46bcec"},{"visibility":"on"}]}],
                zoomControl: true,
                mapTypeControl: false,
                scaleControl: false,
                streetViewControl: false,
                rotateControl: false
            });

            map.addMarker({
                lat: lat,
                lng: lng,
                title: 'PHPers',
                icon: 'http://phpers.pl/favicon.ico',
                infoWindow: {
                    content: '<p>PHPers</p>'
                }
            });
        }

    </script>

